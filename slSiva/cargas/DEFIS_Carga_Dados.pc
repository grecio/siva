/****************************************/
/* Calcula o D�gito de Controle do CNPJ */
/****************************************/
char *CalculaDigitoCnpj (char *cnpj)
{ 
int    num    = 2;  /*  int    num    = 2 */
int    prod   = 0;
int    i      = 0;
int    dig    = 0;
int    auxnum = 0;
int    nauxcpf = 0;
char   auxcpf[1];
	 
  for(i = 11; i >= 0; i--)
    {
     auxcpf[0]  = cnpj[i];
 
     nauxcpf    = atol(auxcpf);

     prod = prod + num *  nauxcpf;
     
	 num = num + 1;
	 
     if (num == 10)  
         num = 2;  

    }
 
  prod = prod % 11;
    
  if (prod < 2) 
      dig = 0;
  else
      dig = 11 - prod;
    
  sprintf(calcdigcnpj,"%s%d",cnpj,dig);
  
  num  = 2;  
  prod = 0;
  dig  = 0;
  
  for(i = 12; i >= 0; i--)
     {
     auxcpf[0]  = calcdigcnpj[i];
     nauxcpf    = atol(auxcpf);

      prod = prod + num * nauxcpf;
      
	  num = num + 1;
	  
	  auxnum = num % 10;
      
	  if (auxnum == 0)
          num = 2;

    }	

  prod = prod % 11;
  if (prod < 2)
      dig = 0;
  else
      dig = 11 - prod;
    
  sprintf(calcdigcnpj,"%s%d",calcdigcnpj,dig);
  
  return calcdigcnpj;

}

