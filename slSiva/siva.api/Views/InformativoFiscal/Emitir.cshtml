                                                    <div class="col-sm-10">ENERGIA, ÁGUA E TELEFONE</div>
                                                    <div class="col-sm-1">[17.1]</div>
                                                    <div class="col-sm-1">@Model.Informativo.CAMPO_171.ToString("C2")</div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-sm-10">ALUGUEL</div>
