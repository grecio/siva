    <!-- Custom Theme Style-->
    <link href="~/Content/build/css/custom.min.css" rel="stylesheet">

    <!-- FullCalendar -->
    <link href="~/Content/vendors/fullcalendar/dist/fullcalendar.min.css" rel="stylesheet">

    <!--calendar-->
