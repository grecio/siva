                    <!-- /menu footer buttons-->
                </div>
            </div>
            <!-- top navigation-->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav role="navigation">
                        <div class="nav toggle"><a id="menu_toggle"><i class="fa fa-bars"></i></a></div>
                        <ul class="nav navbar-nav navbar-right">
                            <li>
